{
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [{"type": "host", "value": "www.i-burdukov.ru"}],
      "destination": "https://i-burdukov.ru/:path*",
      "statusCode": 301
    },
    {
      "source": "/:path*",
      "has": [{"type": "host", "value": "(?<host>.*\\.vercel\\.app)"}],
      "destination": "https://i-burdukov.ru/:path*",
      "statusCode": 301
    }
  ]
}
