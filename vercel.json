{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "(?<host>.*\\.vercel\\.app)"
        }
      ],
      "destination": "https://i-burdukov.ru/:path*",
      "permanent": true
    }
  ]
}

